.cpwhat .upSmartGradient{
background: white;
background: url(data:image/svg+xml;base64,);
background: -moz-linear-gradient(top, white 1%, #EDEDED 50%, #D6D6D6 99%);
background: -webkit-gradient(linear, left top, left bottom, color-stop(1%,white), color-stop(50%,#EDEDED), color-stop(99%,#D6D6D6));
background: -webkit-linear-gradient(top, white 1%,#EDEDED 50%,#D6D6D6 99%);
background: -o-linear-gradient(top, white 1%,#EDEDED 50%,#D6D6D6 99%);
background: -ms-linear-gradient(top, white 1%,#EDEDED 50%,#D6D6D6 99%);
background: linear-gradient(top, white 1%,#EDEDED 50%,#D6D6D6 99%);
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#d6d6d6',GradientType=0 );
}

