.cpwhat .horizGradient{
background: rgb(214,214,214); /* Old browsers */
background: -moz-linear-gradient(left,  rgba(214,214,214,1) 43%, rgba(237,237,237,1) 74%, rgba(255,255,255,1) 98%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, right top, color-stop(43%,rgba(214,214,214,1)), color-stop(74%,rgba(237,237,237,1)), color-stop(98%,rgba(255,255,255,1))); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(left,  rgba(214,214,214,1) 43%,rgba(237,237,237,1) 74%,rgba(255,255,255,1) 98%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(left,  rgba(214,214,214,1) 43%,rgba(237,237,237,1) 74%,rgba(255,255,255,1) 98%); /* Opera 11.10+ */
background: -ms-linear-gradient(left,  rgba(214,214,214,1) 43%,rgba(237,237,237,1) 74%,rgba(255,255,255,1) 98%); /* IE10+ */
background: linear-gradient(left,  rgba(214,214,214,1) 43%,rgba(237,237,237,1) 74%,rgba(255,255,255,1) 98%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d6d6d6', endColorstr='#ffffff',GradientType=1 ); /* IE6-9 */
}

.linkGradient{
background: rgb(255,255,255); /* Old browsers */
background: -moz-linear-gradient(left,  rgba(255,255,255,1) 16%, rgba(237,237,237,1) 63%, rgba(214,214,214,1) 88%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, right top, color-stop(16%,rgba(255,255,255,1)), color-stop(63%,rgba(237,237,237,1)), color-stop(88%,rgba(214,214,214,1))); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(left,  rgba(255,255,255,1) 16%,rgba(237,237,237,1) 63%,rgba(214,214,214,1) 88%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(left,  rgba(255,255,255,1) 16%,rgba(237,237,237,1) 63%,rgba(214,214,214,1) 88%); /* Opera 11.10+ */
background: -ms-linear-gradient(left,  rgba(255,255,255,1) 16%,rgba(237,237,237,1) 63%,rgba(214,214,214,1) 88%); /* IE10+ */
background: linear-gradient(left,  rgba(255,255,255,1) 16%,rgba(237,237,237,1) 63%,rgba(214,214,214,1) 88%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#d6d6d6',GradientType=1 ); /* IE6-9 */
}

